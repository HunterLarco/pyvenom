{
  "name": "pyvenom",
  "scripts": {
    "start": "dev_appserver.py dispatch.yaml app/app.yaml py/app.yaml default/app.yaml api/app.yaml",
    "console": "open http://localhost:8000/console",
    "datastore": "open http://localhost:8000/datastore",
    "deploy:api": "appcfg.py update api",
    "deploy:app": "appcfg.py update app",
    "deploy:py": "appcfg.py update py",
    "deploy:default": "appcfg.py update default",
    "deploy:dispatch": "appcfg.py update_dispatch ."
  }
}
